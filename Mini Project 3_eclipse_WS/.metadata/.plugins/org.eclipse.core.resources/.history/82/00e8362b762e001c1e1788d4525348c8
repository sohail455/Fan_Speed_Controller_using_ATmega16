#include"dc_motor.h"
#include"gpio.h"
/*******************************************************************************
 *                      Functions Definitions                                  *
 *******************************************************************************/
void DcMotor_Init(void)
{
	GPIO_setupPinDirection(INPUT1_PORT, INPUT1_PIN, OUTPUT_PIN);
}
